{
    "workbench.colorTheme": "Habamax",
    "workbench.productIconTheme": "icons-carbon",
    "editor.fontFamily": "'GohuFont 11 Nerd Font Mono'",
    "editor.fontSize": 16,
    "files.autoSave": "afterDelay",
    "editor.wordWrap": "on",
    "workbench.commandPalette.experimental.suggestCommands": true,
    "workbench.commandPalette.preserveInput": true,
    "workbench.list.openMode": "doubleClick",
    "workbench.list.smoothScrolling": true,
    "workbench.localHistory.maxFileEntries": 100,
    "window.customTitleBarVisibility": "auto",
    "workbench.activityBar.location": "bottom",
    "workbench.colorCustomizations": {
        "[Habamax]": {
            "editor.background": "#000",
            "sideBar.background": "#000",
            "editorInfo.background": "#000",
            "editor.lineHighlightBackground": "#000",
            "editorWidget.background": "#000",
            "sideBarSectionHeader.background": "#000",
            "sideBar.dropBackground": "#000",
            "badge.background": "#000",
            "banner.background": "#000",
            "terminal.background": "#000",
            "editorPane.background": "#000",
            "editor.foldBackground": "#000",
            "editorGutter.background": "#000",
            "tab.inactiveBackground": "#000",
            "window.activeBorder": "#000",
            "titleBar.activeBackground": "#000",
            "activityBar.background": "#000",
            "statusBar.background": "#000",
            "breadcrumb.background": "#000",
            "debugToolBar.background": "#000",
            "toolbar.activeBackground": "#000",
            "menu.background": "#000",
            "panel.background": "#000",
            "list.dropBackground": "#000",
            "input.background": "#000",
            "quickInput.background": "#000",
            "sideBarTitle.background": "#000",
            "welcomePage.background": "#000",
            "background": "#000",
        }
    },
    "editor.minimap.enabled": false,
    "workbench.editor.showTabs": "none"
}